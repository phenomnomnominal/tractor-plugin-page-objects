{
  "name": "@tractor-plugins/page-objects",
  "version": "0.5.0",
  "description": "tractor plugin for manipulating Page Objecs",
  "author": "Craig Spence <craigspence0@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/phenomnomnominal/tractor-plugin-page-objects.git"
  },
  "bugs": {
    "url": "https://github.com/phenomnomnominal/tractor-plugin-page-objects/issues",
    "email": "craigspence0@gmail.com"
  },
  "main": "dist/index.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run clean && npm run lint && npm run cover && npm run compile && npm run bundle",
    "bundle": "webpack --config webpack.config.js",
    "clean": "rimraf dist && rimraf reports",
    "compile": "babel src/ --out-dir dist/ --ignore *.spec.js,src/tractor/client",
    "cover": "istanbul cover --config .istanbulrc.json node_modules/mocha/bin/_mocha -- --opts ./.mocharc 'src/**/*.spec.js'",
    "lint": "eslint src",
    "test": "mocha --opts ./.mocharc 'src/**/*.spec.js'",
    "pretest:e2e": "npm run webdriver",
    "test:e2e": "protractor ./tractor/protractor.conf.js --baseUrl=\"http://localhost:4321\"",
    "tractor": "tractor start",
    "tractor:init": "tractor init",
    "tractor:test": "tractor start --config=\"./test.tractor.conf.js\"",
    "watch": "npm-watch",
    "webdriver": "webdriver-manager update"
  },
  "watch": {
    "build": "src/**/*.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.39",
    "@babel/core": "^7.0.0-beta.39",
    "@babel/plugin-proposal-export-default-from": "^7.0.0-beta.39",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.39",
    "@babel/preset-env": "^7.0.0-beta.39",
    "@babel/register": "^7.0.0-beta.39",
    "@tractor-plugins/cucumber": "^0.4.0",
    "@tractor/cli": "^1.0.0-alpha.6",
    "@tractor/core": "^1.0.0-alpha.6",
    "@tractor/unit-test": "^1.0.0-alpha.6",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^8.0.0-beta.0",
    "camel-case": "^3.0.0",
    "css-loader": "^0.28.9",
    "escodegen": "^1.9.0",
    "eslint": "^4.17.0",
    "html-loader": "^0.5.5",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "^5.0.0",
    "ng-annotate-loader": "^0.6.1",
    "npm-watch": "^0.3.0",
    "rimraf": "^2.6.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.6.0"
  },
  "dependencies": {
    "@tractor-plugins/browser": "^0.2.0",
    "babel-polyfill": "^6.26.0",
    "bluebird": "^3.5.0",
    "esprima": "^4.0.0",
    "esquery": "^1.0.0",
    "glob": "^7.1.2",
    "graceful-fs": "^4.1.11",
    "pascal-case": "^2.0.1"
  },
  "peerDependencies": {
    "@tractor/core": "^1.0.0-alpha.6"
  }
}
